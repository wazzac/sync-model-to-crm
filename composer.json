{
    "name": "wazza/sync-model-to-hubspot",
    "description": "Helper package to setup an easy and out of the box Laravel Model (i.e. User, Entity, etc.) to HubSpot object (i.e. Contact, Company, etc.) sync process.",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/support": "~10",
        "guzzlehttp/guzzle": "^7.4",
        "hubspot/api-client": "^10.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20.0",
        "phpunit/phpunit": "~10",
        "orchestra/testbench": "~8",
        "mockery/mockery": "^1.2",
        "laravel/legacy-factories": "~1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wazza\\SyncModelToHubspot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wazza\\SyncModelToHubspot\\": "src/",
            "Wazza\\SyncModelToHubspot\\Tests\\": "tests/"
        }
    },
    "exstra": {
        "laravel": {
            "providers": [
                "Wazza\\SyncModelToHubspot\\Providers\\SyncModelToHubspotServiceProvider"
            ]
        }
    },
    "authors": [{
        "name": "Warren Coetzee",
        "email": "warren.coetzee@gmail.com",
        "homepage": "https://www.wazzac.dev"
    }],
    "minimum-stability": "stable"
}
